{
    "manifest_version": 3,
    "name": "GeoSolvr",
    "version": "0.1.0",
    "author": "k4yp",
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": ["webRequest"],
    "host_permissions": ["<all_urls>"]
}
